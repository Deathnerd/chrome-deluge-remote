{
	"name": "Remote Deluge",
	"description": "Provides a quick overview of torrent status for Deluge.",
	"version": "1.1.0",
	"icons": {
		"128": "images/icons/128.png",
		"64": "images/icons/64.png",
		"32": "images/icons/32.png",
		"16": "images/icons/16.png"
	},
	"web_accessible_resources": [
		"images/icons/16.png",
		"images/icons/16_green.png"
	],
	"default_locale": "en",
	"background": {
		"scripts": [
			"js/libs/jquery-2.1.3.min.js",
			"js/global_options.js",
			"js/deluge.js",
			"js/background.js"
		],
		"persistent": true
	},
	"options_page": "options.html",
	"browser_action": {
		"default_icon": "images/icons/deluge.png",
		"default_title": "__MSG_browser_title_disabled__",
		"default_popup": "popup.html"
	},
	"content_scripts": [
		{
			"matches": ["http://*/*", "https://*/*"],
			"js": ["js/libs/jquery-2.1.3.min.js", "js/global_options.js", "js/add_torrent.js"]
		}
	],
	"permissions": [
		 "tabs", "contextMenus", "storage", "http://*/*", "https://*/*"
	],
	"manifest_version": 2
}
